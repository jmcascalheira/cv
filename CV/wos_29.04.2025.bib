@article{ WOS:000642208600008,
Author = {Haws, Jonathan A. and Benedetti, Michael M. and Talamo, Sahra and Bicho,
   Nuno and Cascalheira, Joao and Ellis, M. Grace and Carvalho, Milena M.
   and Friedl, Lukas and Pereira, Telmo and Zinsious, Brandon K.},
Title = {The early Aurignacian dispersal of modern humans into westernmost
   Eurasia},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2020},
Volume = {117},
Number = {41},
Pages = {25414-25422},
Month = {OCT 13},
Abstract = {Documenting the first appearance of modern humans in a given region is
   key to understanding the dispersal process and the replacement or
   assimilation of indigenous human populations such as the Neanderthals.
   The Iberian Peninsula was the last refuge of Neanderthal populations as
   modern humans advanced across Eurasia. Here we present evidence of an
   early Aurignacian occupation at Lapa do Picareiro in central Portugal.
   Diagnostic artifacts were found in a sealed stratigraphic layer dated
   41.1 to 38.1 ka cal BP, documenting a modern human presence on the
   western margin of Iberia similar to 5,000 years earlier than previously
   known. The data indicate a rapid modern human dispersal across southern
   Europe, reaching the westernmost edge where Neanderthals were thought to
   persist. The results support the notion of a mosaic process of modern
   human dispersal and replacement of indigenous Neanderthal populations.},
