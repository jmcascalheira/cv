@article{ WOS:000644886100002,
Author = {Taylor, Ruth and Garcia-Rivero, Daniel and Goncalves, Celia and
   Cascalheira, Joao and Bicho, Nuno},
Title = {The Early Neolithic at the Muge Shellmiddens (Portugal): Analysis and
   Review of the Ceramic Evidence from Cabeco da Amoreira},
Journal = {EUROPEAN JOURNAL OF ARCHAEOLOGY},
Year = {2021},
Volume = {24},
Number = {2},
Pages = {156-179},
Month = {MAY},
Abstract = {This article, on the Early Neolithic pottery from the Cabeco da Amoreira
   shellmidden in the Muge region of central Portugal, presents a detailed
   review of the evidence to date and a systematic analysis of the
   decorative and mineralogical characteristics of the stratified and
   radiocarbon-dated ceramic assemblage. A homogenous pottery manufacturing
   tradition seems to be present right from the beginning, including both
   local and non-local ceramics. The authors formulate a working hypothesis
   on the geographic origin of the exogenous pottery, which contributes to
   the discussion of the dynamics of mobility and social networks in the
   Neolithization of south-western Europe.},
