@dataset{rose_2024,
  title = {Research Compendium for the Paper "{{Mapping}} Lateral Stratigraphy at {{Palaeolithic}} Surface Sites: A Case Study from {{Dhofar}}, {{Oman}}"},
  shorttitle = {Research Compendium for the Paper "{{Mapping}} Lateral Stratigraphy at {{Palaeolithic}} Surface Sites},
  author = {Rose, Jeffrey I. and Li, Li and Cascalheira, Jo\~ao},
  date = {2024},
  publisher = {OSF},
  doi = {10.17605/OSF.IO/RNYFB},
  url = {https://osf.io/rnyfb/},
  urldate = {2024-10-30},
  abstract = {Open-air anthropogenic accumulations of chipped stone debris are a common feature of arid landscapes. Despite their prevalence, however, such archives are often disregarded as uninformative and unreliable. Across the canyonlands of Dhofar, southern Oman, lithic surface scatters are virtually ubiquitous, including extensive multi-component workshops found in proximity to chert outcrops. In many cases, such sites display a range of chronologically diagnostic features associated w 45 ith different lithic taphonomic states, with more heavily weathered artifacts seemingly distributed farther away from the present chert outcrop. As such, we posit that post-depositional surface modifications and spatial patterns evident on chipped stone materials are a function of site formation processes and may, under certain conditions, provide relative chronological information where absolute dating methods are limited. The study presented here tests this hypothesis by mapping spatial distribution and lithic taphonomy of artifacts at a series of multi-component surface sites in Dhofar spanning Lower, Middle, and Upper/Late Palaeolithic periods. Results confirm the hypothesized patterns, with a tendency for more heavily weathered artifacts to be distributed farther away from the chert exposure, compared to less weathered specimens. This observation provides insight into natural/anthropogenic mechanisms of surface site formation and permits the study of relative technological change. While these principles appear to be effective predictive markers of age at the broadest scale, their usefulness for analyzing assemblage variability within smaller intervals of time is yet to be determined. In the future, lithic taphonomy recording systems should seek techniques to quantify surface modifications to produce empirical observations.}
}

@article{rose_2025,
  title = {Mapping Lateral Stratigraphy at {{Palaeolithic}} Surface Sites: {{A}} Case Study from {{Dhofar}}, {{Oman}}},
  shorttitle = {Mapping Lateral Stratigraphy at {{Palaeolithic}} Surface Sites},
  author = {Rose, Jeffrey I. and Hilbert, Yamand\'u H. and Usyk, Vitaly I. and Bebber, Michelle R. and Beshkani, Amir and Buchanan, Briggs and Cascalheira, Jo\~ao and Chlachula, Dominik and Dellmour, Rudolf and Eren, Metin I. and Garba, Roman and Hallinan, Emily and Li, Li and Walker, Robert S. and Marks, Anthony E.},
  date = {2025},
  journaltitle = {Journal of Archaeological Science},
  shortjournal = {Journal of Archaeological Science},
  volume = {173},
  pages = {106117},
  issn = {0305-4403},
  doi = {10.1016/j.jas.2024.106117},
  url = {https://www.sciencedirect.com/science/article/pii/S0305440324001857},
  urldate = {2024-11-22},
  abstract = {Open-air accumulations of chipped stone debris are a common feature in arid landscapes, yet despite their prevalence, such archives are often dismissed as uninformative or unreliable. In the canyonlands of Dhofar, southern Oman, lithic surface scatters are nearly ubiquitous, including extensive, multi-component workshops associated with chert outcrops. These sites typically display chronologically diagnostic features that correspond to distinct taphonomic states, which in turn appear linked to spatial distribution, with more heavily weathered artifacts often found farther from the chert outcrops. We propose that post-depositional modifications and spatial distributions of chipped stone artifacts reflect site formation processes and, under certain conditions, may provide relative chronological information when absolute dating methods are unavailable. Our study tests this hypothesis by mapping artifact distribution and lithic taphonomy across a series of surface sites in southern Oman, spanning the Lower, Middle, and Upper/Late Palaeolithic periods. The results largely support our model, offering valuable insights into surface site formation and technological change over time. While these findings serve as broad predictive markers for age, their applicability for analyzing finer-scale assemblage variability remains to be determined. Future taphonomic recording systems should aim to quantify surface modifications to enhance replicability for such studies.},
  file = {/Users/jmcascalheira/Zotero/storage/8HMNMXDV/Rose et al. - 2025 - Mapping lateral stratigraphy at Palaeolithic surface sites A case study from Dhofar, Oman.pdf;/Users/jmcascalheira/Zotero/storage/IM4B7UPA/S0305440324001857.html}
}

@online{ruether_2021,
  title = {{{SPIN}} - {{Species}} by {{Proteome INvestigation}}},
  author = {Ruether, Patrick Leopold and Husic, Immanuel Mirnes and Bangsgaard, Pernille and Gregersen, Kristian Murphy and Pantmann, Pernille and Carvalho, Milena and Godinho, Ricardo Miguel and Friedl, Lukas and Cascalheira, Joao and Joerkov, Marie Louise Schjellerup and Benedetti, Michael and Haws, Jonathan and Bicho, Nuno and Welker, Frido and Cappellini, Enrico and Olsen, Jesper Velgaard},
  date = {2021},
  eprinttype = {bioRxiv},
  eprintclass = {New Results},
  doi = {10.1101/2021.02.23.432520},
  url = {https://www.biorxiv.org/content/10.1101/2021.02.23.432520v1},
  abstract = {{$<$}p{$>$}Genetic species determination has become an indispensable tool in forensics, archaeology, ecology, and food authentication. The available methods are either suited for detecting a single taxon across many samples or for screening a wide range of species across a few samples. Here, we introduce "Species by Proteome INvestigation" (SPIN), a proteomics workflow capable of querying over 150 mammalian species in 7.2 minutes of mass spectrometry (MS) analysis. Streamlined and automated sample preparation by protein aggregation capture, high-speed chromatography and data-independent acquisition, and a confident species inference algorithm facilitate processing hundreds of samples per day. We demonstrate the correct classification of known references, reproducible species identification in degraded Iron-Age material from Scandinavia, and test the limits of our methods with Middle and Upper Palaeolithic bones from Southern European sites with late Neanderthal occupation. While this initial study is focused on modern and archaeological mammalian bone, SPIN will be open and expandable with other biological tissues and taxa.{$<$}/p{$>$}},
  language = {en},
  pubstate = {prepublished},
  file = {/Users/jmcascalheira/ICArEHB Dropbox/João Cascalheira/[6] PERSONAL/Zoterodb/Ruether et al_2021_SPIN - Species by Proteome INvestigation.pdf;/Users/jmcascalheira/Zotero/storage/4PQKNV8L/2021.02.23.html}
}

@article{ruther_2022,
  title = {{{SPIN}} Enables High Throughput Species Identification of Archaeological Bone by Proteomics},
  author = {R\"uther, Patrick Leopold and Husic, Immanuel Mirnes and Bangsgaard, Pernille and Gregersen, Kristian Murphy and Pantmann, Pernille and Carvalho, Milena and Godinho, Ricardo Miguel and Friedl, Lukas and Cascalheira, Jo\~ao and Taurozzi, Alberto John and J\o rkov, Marie Louise Schjellerup and Benedetti, Michael M. and Haws, Jonathan and Bicho, Nuno and Welker, Frido and Cappellini, Enrico and Olsen, Jesper Velgaard},
  date = {2022},
  journaltitle = {Nature Communications},
  shortjournal = {Nat Commun},
  volume = {13},
  number = {1},
  pages = {2458},
  issn = {2041-1723},
  doi = {10.1038/s41467-022-30097-x},
  url = {https://www.nature.com/articles/s41467-022-30097-x},
  urldate = {2023-01-20},
  abstract = {Abstract             Species determination based on genetic evidence is an indispensable tool in archaeology, forensics, ecology, and food authentication. Most available analytical approaches involve compromises with regard to the number of detectable species, high cost due to low throughput, or a labor-intensive manual process. Here, we introduce ``Species by Proteome INvestigation'' (SPIN), a shotgun proteomics workflow for analyzing archaeological bone capable of querying over 150 mammalian species by liquid chromatography-tandem mass spectrometry (LC-MS/MS). Rapid peptide chromatography and data-independent acquisition (DIA) with throughput of 200 samples per day reduce expensive MS time, whereas streamlined sample preparation and automated data interpretation save labor costs. We confirm the successful classification of known reference bones, including domestic species and great apes, beyond the taxonomic resolution of the conventional peptide mass fingerprinting (PMF)-based Zooarchaeology by Mass Spectrometry (ZooMS) method. In a blinded study of degraded Iron-Age material from Scandinavia, SPIN produces reproducible results between replicates, which are consistent with morphological analysis. Finally, we demonstrate the high throughput capabilities of the method in a high-degradation context by analyzing more than two hundred Middle and Upper Palaeolithic bones from Southern European sites with late Neanderthal occupation. While this initial study is focused on modern and archaeological mammalian bone, SPIN will be open and expandable to other biological tissues and taxa.},
  language = {en},
  file = {/Users/jmcascalheira/ICArEHB Dropbox/João Cascalheira/[6] PERSONAL/Zoterodb/Rüther et al_2022_SPIN enables high throughput species identification of archaeological bone by.pdf;/Users/jmcascalheira/ICArEHB Dropbox/João Cascalheira/[6] PERSONAL/Zoterodb/Rüther et al_2022_SPIN enables high throughput species identification of archaeological bone by2.pdf;/Users/jmcascalheira/Zotero/storage/THV93JL4/s41467-022-30097-x.html}
}

