  author = {Cascalheira, João and Alcaraz-Castaño, Manuel and Alcolea-González, Javier and family=Andrés-Herrero, given=Maria, prefix=de, useprefix=true and Arrizabalaga, Alvaro and Aura Tortosa, J. Emili and Garcia-Ibaibarriaga, Naroa and Iriarte-Chiapusso, Maria-Jose},
  date = {2021},
  journaltitle = {Quaternary International},
