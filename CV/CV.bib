@article{carvalho_2022a,
  title = {Neanderthal Palaeoecology in the Late {{Middle Palaeolithic}} of Western {{Iberia}}: A Stable Isotope Analysis of Ungulate Teeth from {{Lapa}} Do {{Picareiro}} ({{Portugal}})},
  shorttitle = {Neanderthal Palaeoecology in the Late {{Middle Palaeolithic}} of Western {{Iberia}}},
  author = {Carvalho, Milena and Jones, Emily Lena and Ellis, M. Grace and Cascalheira, Jo\~ao and Bicho, Nuno and Meiggs, David and Benedetti, Michael and Friedl, Lukas and Haws, Jonathan},
  date = {2022},
  journaltitle = {Journal of Quaternary Science},
  volume = {37},
  number = {2},
  pages = {300--319},
  issn = {1099-1417},
  doi = {10.1002/jqs.3363},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/jqs.3363},
  urldate = {2022-03-02},
  abstract = {Adaptation to Late Pleistocene climate change is an oft-cited potential contributor to Neanderthal disappearance in Eurasia. Accordingly, research on Neanderthal behaviour -- including subsistence strategies, mobility, lithic technology, raw material procurement and demography -- often focuses on linking changes observable in the archaeological record to specific phases of climate and environmental change. However, these correspondences are often tenuous because palaeoclimatic and archaeological records are rarely available on the same scale. In Iberia, a critical location for understanding the demise of Neanderthals, some research indicates that Neanderthal populations were unable to recover from environmental degradations known as Heinrich Events, while other studies suggest that enclaves of Neanderthal populations survived for several millennia longer in refugial zones. Here, we present a palaeoenvironmental reconstruction study using analysis of {$\delta$}13C and {$\delta$}18O of herbivore tooth enamel recovered from two Mousterian deposits at Lapa do Picareiro, a site located in Portuguese Estremadura. We then use these data, combined with other site-based palaeoenvironmental indicators, to assess whether central Portugal acted as a refugium during periods of unfavourable climate, and to test whether Neanderthals in Portuguese Estremadura reorganised their mobility strategies after severe climate episodes.},
  language = {en},
  file = {/Users/jmcascalheira/ICArEHB Dropbox/João Cascalheira/[6] PERSONAL/Zoterodb/Carvalho et al_2022_Neanderthal palaeoecology in the late Middle Palaeolithic of western Iberia.pdf;/Users/jmcascalheira/ICArEHB Dropbox/João Cascalheira/[6] PERSONAL/Zoterodb/Carvalho et al_2022_Neanderthal palaeoecology in the late Middle Palaeolithic of western Iberia2.pdf;/Users/jmcascalheira/Zotero/storage/IU6BASY2/jqs.html}
}

