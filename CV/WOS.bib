@article{ ISI:000637160300003,
Author = {Cascalheira, Joao and Alcaraz-Castano, Manuel and Alcolea-Gonzalez,
   Javier and Andres-Herrero, Maria de and Arrizabalaga, Alvaro and
   Tortosa, J. Emili Aura and Garcia-Ibaibarriaga, Naroa and
